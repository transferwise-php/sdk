{
    "name": "transferwise-php/sdk",
    "description": "PHP binding for TransferWise API v1",
    "type": "library",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ha Phan",
            "email": "thanhha.work@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TransferWise\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TranferWise\\Test\\": "tests/unit/"
        }
    },
    "minimum-stability": "dev",
